{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": "container",
  "height": 500,
  "projection": {"type": "equirectangular"},
  "params": [
    {
      "name": "total_Above",
      "value": 0,
      "bind": {
        "input": "range",
        "min": 0,
        "max": 41000,
        "step": 100,
        "name": "Minimum Population: "
      }
    }
  ],
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/imdrones3/FIT3179/main/week9hw/data/australia.topojson",
        "format": {"type": "topojson", "feature": "STE11aAust"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/imdrones3/FIT3179/main/Vis2/data/universitieslatlong.csv"
      },
      "transform": [{"filter": "datum.total > total_Above"}],
      "mark": {"type": "point", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "longitude", "type": "quantitative"},
        "latitude": {"field": "latitude", "type": "quantitative"},
        "size": {
          "field": "total",
          "type": "quantitative",
          "scale": {
            "type": "threshold",
            "domain": [1000, 5000, 7500, 15000, 30000],
            "range": [50, 100, 200, 400, 1000, 2000]
          }
        },
        "color": {
          "field": "state",
          "type": "nominal",
          "scale": {
            "domain": [
              "NSW",
              "VIC",
              "QLD",
              "WA",
              "SA",
              "TAS",
              "NT",
              "ACT",
              "Multistate"
            ],
            "range": [
              "#cab2d6",
              "#ff7f00",
              "#fdbf6f",
              "#e31a1c",
              "#fb9a99",
              "#33a02c",
              "#b2df8a",
              "#1f78b4",
              "#a6cee3"
            ]
          }
        },
        "opacity": {"value": 1}
      }
    }
  ],
  "config": {}
}